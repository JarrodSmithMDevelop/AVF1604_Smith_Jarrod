
var getGeo = fuction(){
	if (Ti.Geolocation.purpose = "Application needs your location cordinates";
		Ti.Geolocation.getCurrentPosition(function(e)
			console.log(e);
			
	var geo = Ti.Geolocation.getCurrentPosition
		var lat = e.coords.latitude;
		var lng = e.coords.longitude;
        var ui = require("ui");
		ui.buildUi(lat, lng); 
		)
	)} else {
		alert("Please enable location settings on device!")
	};

exports.runGetGeo = getGeo;
